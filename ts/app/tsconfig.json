{
  "angularCompilerOptions": {
    "googleClosureOutput": true
  },
  "compilerOptions": {
    "charset": "UTF-8",
    "experimentalDecorators": true,
    "module": "commonjs",
    "newLine": "lf",
    "noEmitHelpers": true,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "outDir": "../../js/app/ts",
    "pretty": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es6"
  }
}

